<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>SampleWebApp</display-name>
  <welcome-file-list>
    	<welcome-file>View/FirstPage.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
  <servlet-name>MyFirstApp</servlet-name>
  <servlet-class>com.AlgoFocus.MyApplication.Controller.FirstAppController</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>MyFirstApp</servlet-name>
  <url-pattern>/FirstApplication</url-pattern>
  </servlet-mapping>
  
</web-app>